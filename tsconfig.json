{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "esModuleInterop": true,
      "sourceMap": true,
      "outDir": "./dist",
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "include": ["server.ts"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }
  